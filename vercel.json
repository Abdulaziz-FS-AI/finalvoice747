{
  "version": 2,
  "builds": [
    {
      "src": "server-fixed.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-fixed.js"
    },
    {
      "src": "/health",
      "dest": "/server-fixed.js"
    },
    {
      "src": "/dashboard",
      "dest": "/server-fixed.js"
    },
    {
      "src": "/",
      "dest": "/server-fixed.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "server-fixed.js": {
      "maxDuration": 30
    }
  }
}